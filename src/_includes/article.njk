--- 
layout: 'base.njk'
---
       
<article class="main-article  max-w-7xl px-8 mx-auto"> <!--prose prose-lg-->
<div class="container container--narrow flow ">
  
    <h1 class="article-title">
      {{ title }}
    </h1>
    <figure class="main-article__figure">
      <img src="{{ image }}" alt="{{ imageAlt }}">
      <figcaption>
        {{ imageAlt }}
      </figcaption>
    </figure>

    {{ content | safe }}
    
  
</div>
</article>

